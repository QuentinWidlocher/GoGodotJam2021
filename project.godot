[application]

config/name="GoGodotJam2021"
run/main_scene="res://Scenes/UI/Menu/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

IdleSystem="*res://Scripts/Singletons/IdleSystem/IdleSystem.cs"
SceneSwitcher="*res://Scripts/Singletons/SceneSwitcher/SceneSwitcher.cs"
PauseScreen="*res://Scenes/UI/Menu/PauseScreen.tscn"
Player="*res://Scenes/Player/Player.tscn"
SaveSystem="*res://Scripts/Singletons/SaveSystem.cs"
StatSystem="*res://Scripts/Singletons/StatSystem/StatSystem.cs"

[display]

window/size/width=850
window/size/height=480

[editor_plugins]

enabled=PoolStringArray( "res://addons/rmsmartshape/plugin.cfg" )

